{
  "name": "@roll-stack/web-app",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --port 3501",
    "dev:compose": "docker compose -f ./../../docker/web-app/docker-compose.yaml up -d",
